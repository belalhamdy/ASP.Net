@model IEnumerable<NT_Project.Models.Interaction>

@{
    ViewBag.Title = "GetInteractions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>GetInteractions</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ApplicationUser.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Post.PostId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActionType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ApplicationUser.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Post.PostId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ActionType)
        </td>

    </tr>
}

</table>
